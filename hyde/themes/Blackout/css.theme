/* Global Styling */
* {
        font-family: "Monocraft";
        font-weight: normal;
        /*font-size: 13.9px;*/
        font-size: 13px;
        min-height: 12px;
}

@import "theme.css";

.main {
        background: @main-bg;
        /* background: rgba; */
        /* background-image: url */
}

.workspaces {
        background: @main-bg;
        /* background: rgba(0,0,0,0); */
        /* background-image: url("/home/q/.config/hyde/themes/Darkpuccin/wallpapers/dodge left.png") */
}

#custom-updates {
        color: #add8e6;
        /* background: #29313e; */
        /* background: @main-bg; */
        margin-top: 0px;
        margin-bottom: 0px;
}

#custom-recording {
        color: #f38ba8;
}

#workspaces button.empty {
        color: @empty-fg;
        background-color: @main-bg;
        border-radius: 0px;
        margin-top: -6px;
        margin-bottom: -3px;
        margin-left: 0px;
        margin-right: 0px;
        padding: 0px;
        padding-top: 0px;
        padding-bottom: 0px;
        padding-left: 4px;
        padding-right: 4px;
        /*animation: ws_normal 20s ease-in-out 1;*/
        animation: none;
}

#workspaces button {
        color: @main-fg;
        background-color: @main-bg;
        border-radius: 0px;
        margin-top: -6px;
        margin-bottom: -3px;
        margin-left: 0px;
        margin-right: 0px;
        padding: 0px;
        padding-top: 0px;
        padding-bottom: 0px;
        padding-left: 4px;
        padding-right: 4px;
        /*animation: ws_normal 20s ease-in-out 1;*/
        animation: none;
}

#workspaces button.active {
        color: @red;
        background: @main-bg;
        animation: ws_active 20s ease-in-out 1;
        transition: all 0.2s cubic-bezier(0.55, -0.68, 0.48, 1.682);
}

#workspaces button:hover {
        color: @module-fg;
        background: @main-bg;
        /*animation: ws_hover 20s ease-in-out 1;*/
        /*transition: all 0.2s cubic-bezier(0.55, -0.68, 0.48, 1.682);*/
        animation: none;
        transition: none;
}

